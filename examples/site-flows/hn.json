{
  "settings": {
    "headed": true,
    "deterministic": true,
    "captureScreenshots": true
  },
  "actions": [
    {
      "type": "navigate",
      "url": "https://news.ycombinator.com",
      "waitUntil": "domcontentloaded"
    },
    {
      "type": "click",
      "target": {
        "kind": "stableRef",
        "value": "href:https://news.ycombinator.com/newest"
      }
    },
    {
      "type": "waitFor",
      "condition": {
        "kind": "selector",
        "selector": "a[href='newest']",
        "state": "visible"
      }
    },
    {
      "type": "snapshot"
    }
  ]
}
