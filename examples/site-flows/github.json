{
  "settings": {
    "headed": true,
    "deterministic": true,
    "captureScreenshots": true
  },
  "actions": [
    {
      "type": "navigate",
      "url": "https://github.com/login",
      "waitUntil": "domcontentloaded"
    },
    {
      "type": "fill",
      "target": {
        "kind": "css",
        "selector": "#login_field"
      },
      "value": "sazen@example.com"
    },
    {
      "type": "waitFor",
      "condition": {
        "kind": "selector",
        "selector": "#login_field",
        "state": "visible"
      }
    },
    {
      "type": "snapshot"
    }
  ]
}
