{
  "settings": {
    "headed": true,
    "deterministic": true,
    "captureScreenshots": true
  },
  "actions": [
    {
      "type": "navigate",
      "url": "https://vite.dev",
      "waitUntil": "domcontentloaded"
    },
    {
      "type": "click",
      "target": {
        "kind": "stableRef",
        "value": "href:https://vite.dev/guide/"
      }
    },
    {
      "type": "waitFor",
      "condition": {
        "kind": "selector",
        "selector": "h1",
        "state": "visible"
      }
    },
    {
      "type": "snapshot"
    }
  ]
}
