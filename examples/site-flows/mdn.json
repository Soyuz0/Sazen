{
  "settings": {
    "headed": true,
    "deterministic": true,
    "captureScreenshots": true
  },
  "actions": [
    {
      "type": "navigate",
      "url": "https://developer.mozilla.org",
      "waitUntil": "domcontentloaded"
    },
    {
      "type": "click",
      "target": {
        "kind": "stableRef",
        "value": "href:https://developer.mozilla.org/en-US/docs/Web/HTML"
      }
    },
    {
      "type": "waitFor",
      "condition": {
        "kind": "selector",
        "selector": "main",
        "state": "visible"
      }
    },
    {
      "type": "snapshot"
    }
  ]
}
